Items you've completed

<% @my_tasks.each do |x| %>
  <div class="row">
        <div class="col s12 m6">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <h4 class="card-title center-align"><%= x.name.titleize %></h4>
              <p>Description: <%= x.description %></p>
              <p>Where: <%= x.location %></p>
              <p>Event Created: <%= x.created_at.strftime("%m/%d/%Y") %></p>
            </div>
            <div class="card-action">
              <a href="#">who created</a>
              <a href="#">who completed it?</a>
            </div>
          </div>
        </div>
      </div>
  <% end %>



